


.show{display:block;}
.hidden{display:none;}

.swiper-tab{  
    display:flex;
    justify-content:space-around;
    width: 100%;  
    border-bottom: 2rpx solid #777777;  
    text-align: center;  
    line-height: 80rpx;}  
.swiper-tab-list{  font-size: 30rpx;  
    display: inline-block;  
    width: 20%;  
    color: #777777;  
}  
.on{ color: #da7c0c;  
    border-bottom: 5rpx solid #da7c0c;}  
  
.swiper-box{ display: block; width: 100%; }  
.swiper-box view{  
    text-align: left;  
}


.swiper_item{
    display:flex;
    background:#fff;
    margin-top:10px;
    margin-left:10rpx;
    margin-right:10rpx;
}
.item_right{
width:100%;
    position:relative;
    overflow:hidden;
}
.my_img{
    width:200rpx;
    height:260rpx;
    display:block;
    margin:0 20rpx 0 0;
}

.my_text{
    width:100%;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    display:inline-block;
     margin-top:10rpx;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #000000;
    line-height: 32rpx;
    
}
.my_tag{
    display:inline-block;
    padding:0 10rpx;
    height:40rpx;
    text-align:center;
    background: #EEEEEE;
    margin-right:10rpx;
    margin-top:10rpx;
}
.my_tag text{
     font-family: PingFangSC-Regular;
    font-size: 22rpx;
    color: #000000;
     line-height:40rpx;
}
.my_date{
    margin-top:20rpx;
}
.my_time{
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #FF5252;
    line-height: 14px;
    margin-top:20rpx;
}
.my_address{
    width:100%;
    position:absolute;
    left:0;
    bottom:0;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #888888;
    line-height: 32rpx;
}
.bottom_li{
    border-bottom:1rpx solid #000;
    color:#000;
}
.hidden{
    display:none;
}
.complete{
    text-align:center;
}
.feed_item{
	    background: rgba(238,238,238,0.80);
    margin-top:10px;
    text-align:left;
}
.feed_my_info{
    display:flex;
}
.feed_img{
    width:33px;
    height:33px;
    border-radius:50%;
     margin:13px 10px 0 16px;
}
.feed_name{
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #2D2D2D;
    line-height: 14px;
    display:block;
     margin-top:14px;
}
.feed_time{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #999999;
    line-height: 12px;
    display:block;
    text-align:left;

}
.feed_content_name{
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #000000;
    line-height: 14px;
   margin:12px 0 0 15px;
}
.feed_show_img{
    display:flex;
    flex-wrap:wrap;
}
.feed_small_img{
    margin:5px 5px;
    width:20%;
}
.feed_collect text{
   font-family: PingFangSC-Regular;
font-size: 14px;
color: #000000;
line-height: 14px;
}
.feed_comment{
	font-family: PingFangSC-Regular;
font-size: 12px;
color: #666666;
line-height: 12px;
text-align:left;
}
.feed_text{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #666666;
}
.my_date{
	font-family: PingFangSC-Regular;
	font-size: 12px;
	color: #888888;
	line-height: 12px;
}
.item_right{
	text-align:left;
	width:100%;
}
.apply_event{
	height:75px;
	margin-bottom:5px;
}
.event_name{
	font-family: PingFangSC-Regular;
font-size: 12px;
color: #999999;
line-height: 12px;
}
.apply_right{
	display:flex;
	text-align:left;

}
.apply_img{
	width:56px;
	height:56px;
}
.apply_title{
	font-family: PingFangSC-Regular;
font-size: 14px;
color: #000000;
line-height: 14px;
margin-top:10px;
}
.apply_time{
	font-family: PingFangSC-Regular;
font-size: 12px;
color: #888888;
line-height: 12px;
margin-top:10px;
}