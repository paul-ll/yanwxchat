page{background:#f7f7f7;}
.fd-hd {
    position: relative;
    width: 690rpx;
    height: 160rpx;
    display: flex;
    bachground:#fff;
    align-content: center;
    overflow: hidden;
    margin: 20rpx 0;
   margin-left:30rpx;
}

.fd-hd:before {
    content: '';
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    z-index: -1;
    width: 100%;
    height: 400rpx;
    background-color: rgba(0, 0, 0, .6);
}

.fd-hd-bg {
    position: absolute;
    z-index: -2;
    left: 0;
    top: 0;
    width: 690rpx;
    height: 160rpx;
    background-size: cover;
    background-position: center;
    filter: blur(30rpx);
    transform: scale(1.2);
}

.fd-favorite {
    box-sizing: border-box;
    position: absolute;
    z-index: 99;
    bottom: 50rpx;
    right: 20rpx;
    width: 160rpx;
    padding: 10rpx 30rpx;
    padding-left: 60rpx;
    font-size: 32rpx;
    line-height: 1;
    border-radius: 30rpx;
    border: 1px solid #fff;
    background-position: 20rpx center;
    background-repeat: no-repeat;
    background-image: url(data:image/png;base64,);
    background-size: 32rpx;
    color: #fff;
    transition: left .3s, color .3s, background-image .3s;
}

.fd-favorite-active {
    right: 20rpx;
    width: 190rpx;
    background-image: url(data:image/png;base64,);
    color: #f4ea2a;
    border: 1px solid #f4ea2a;
}

.fd-favorite-active::before {
    content: '已'
}

.fd-cover {
    z-index: 9999;
    width: 120rpx;
    height: 160rpx;
  
    border-radius: 0rpx;
    box-shadow: 0 30rpx 150rpx rgba(255, 255, 255, .3)
}

