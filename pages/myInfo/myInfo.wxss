page{
    background: rgba(238,238,238,0.80);
}
.container{
    background: rgba(238,238,238,0.80);
    height:100%;
}
.user-info {
    box-sizing: border-box;
    width: 100%;
    height: 160rpx;
    padding: 10rpx 30rpx;
    display: flex;
    justify-content: space-between;
    border-bottom:1rpx solid #E5E5E5;
    background:#fff;
}
.user_box{

    margin-top:32rpx;

}
.user_right{
    margin-top:56rpx;
    color:#ccc;
    font-size: 44rpx;
}
.user-skin {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 60rpx;
    height: 60rpx;
    border: 4rpx solid #fff;
    border-radius: 60rpx;
    background-color: rgba(0, 0, 0, .3);
   
    background-position: center;
    background-repeat: no-repeat;
    background-size: 40rpx;
}

.user-nickname {
    color: #fff;
    font-size: 40rpx;
    text-shadow: 1px 1px 0 rgba(0, 0, 0, .6);
}
.user_text{
    display:flex;
}
.user_at{
    display:flex;
}
.user-sex-male, .user-sex-female {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    margin-bottom: -5rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}
.user_at{
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #333333;
    line-height: 36rpx;
}
.user_name{
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #333333;
    line-height: 48rpx;
}
.user-sex-male {
    background-image: url(data:image/png;base64,);
}

.user-sex-female {
    background-image: url(data:image/png;base64,);
}

.user-avatar {
    width: 95rpx;
    height: 95rpx;
    border-radius: 50%;
    margin-right:20rpx;
    box-shadow: 0 0 300rpx 100rpx rgba(255, 255, 255, .2), 0 1px 0 rgba(255, 255, 255, 0.4);
}

.user-location {
    font-size: 24rpx;
    color: #eee;
    text-shadow: 1px 1px 0 rgba(0, 0, 0, .6);
}
.grids{
    background: rgba(238,238,238,0.80);
    margin-top:10rpx;
    margin-bottom:30rpx;
}



.my_info_active{
    background:#fff;
    width:100%;
    height:65px;
    display:flex;
    justify-content:space-around;
    line-height:65px;
}
.my_info_active_1,.my_info_active_2{
    margin-top:15px;
    font-family: PingFangSC-Medium;
    font-size: 18px;
    color: #000000;
    letter-spacing: 0.36px;
    line-height: 18px;
}
.my_info_active text{
    font-family: PingFangSC-Regular;
font-size: 12px;
color: #999999;
letter-spacing: 0.24px;
line-height: 12px;
display:block;
}
.my_info_flex{
    display:flex;
    width:100%;
    height:46px;
    background:#fff;
    margin-top:10px;

}
.my_info_paid,.my_info_trick{
    position:relative;
}
.my_info_img image{
    width:18px;
    height:18px;
    margin:12px 10px 0 16px;
}
.my_info_name{
    margin-top:16px;
    font-family: PingFangSC-Regular;
font-size: 14px;
color: #333333;
line-height: 14px;
}
.myInfo_arr_tick{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #FF5454;
    line-height: 46px;
    position:absolute;
    right:20px;
    
}
.myInfo_arr_apily{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #FF5454;
    line-height: 46px;
    position:absolute;
    right:20px;
}
.my_info_paid{
    margin-top:10rpx;
}
.login_no{
    margin-top:20rpx;
}






