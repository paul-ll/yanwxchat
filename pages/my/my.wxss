.user-info {
    box-sizing: border-box;
    width: 100%;
    height: auto;
   text-align:center;

    border-bottom:1rpx solid #E5E5E5;
}
.user_box{
    text-align:center;
    margin:0 auto;
    margin-top:32rpx;
    padding-bottom:20rpx;
}
.user_right{
    margin-top:66rpx;
    color:#ccc;
    font-size: 44rpx;
}
.user-skin {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 60rpx;
    height: 60rpx;
    border: 4rpx solid #fff;
    border-radius: 60rpx;
    background-color: rgba(0, 0, 0, .3);
   
    background-position: center;
    background-repeat: no-repeat;
    background-size: 40rpx;
}
.user_intro{
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333333;
    line-height:36rpx;
}
.user-nickname {
    color: #fff;
    font-size: 40rpx;
    text-shadow: 1px 1px 0 rgba(0, 0, 0, .6);
}

.user_at{
    text-align:center;
    display:flex;
    margin-left:40%;
    margin-right:40%;

}
.user-sex-male, .user-sex-female {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    margin-bottom: -5rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}
.user_at{
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #333333;
    line-height: 36rpx;
}
.user_name{
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #333333;
    line-height: 48rpx;
}
.user-sex-male {
    background-image: url(data:image/png;base64,);
}

.user-sex-female {
    background-image: url(data:image/png;base64,);
}

.user-avatar {
    width: 95rpx;
    height: 95rpx;
    border-radius: 50%;
    margin-right:20rpx;
    box-shadow: 0 0 300rpx 100rpx rgba(255, 255, 255, .2), 0 1px 0 rgba(255, 255, 255, 0.4);
}
.user-location {
    font-size: 24rpx;
    color: #eee;
    text-shadow: 1px 1px 0 rgba(0, 0, 0, .6);
}

.swiper-tab{  
    display:flex;
    justify-content:space-around;
    width: 100%;  
    border-bottom: 2rpx solid #777777;  
    text-align: center;  
    line-height: 80rpx;}  
.swiper-tab-list{  font-size: 30rpx;  
    display: inline-block;  
    width: 20%;  
    color: #777777;  
}  
.on{ color: #da7c0c;  
    border-bottom: 5rpx solid #da7c0c;}  
  
.swiper-box{ display: block; width: 100%; }  
.swiper-box view{  
    text-align: left;  
}


.swiper_item{
    height:160rpx;
    display:flex;
    background: rgba(238,238,238,0.80);
    margin-top:10px;
}
.my_img{
    width:60px;
    height:80px;
    display:block;
    margin:0 10px 0 0;
}
.my_text text{
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #000000;
    line-height: 14px;
    margin-top:10px;
}
.my_time{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #888888;
    line-height: 12px;
    margin-top:10px;
}
.my_address{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #888888;
    line-height: 12px;
    margin-top:10px;
}
.feed_my_info{
    display:flex;
}
.feed_img{
    width:33px;
    height:33px;
    border-radius:50%;
     margin:13px 10px 0 16px;
}
.feed_name{
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #2D2D2D;
    line-height: 14px;
    display:block;
     margin-top:14px;
}
.feed_time{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #999999;
    line-height: 12px;
    display:block;
   
}
.feed_content_name{
    font-family: PingFangSC-Regular;
    font-size: 14px;
    color: #000000;
    line-height: 14px;
   margin:12px 0 0 15px;
}
.feed_show_img{
    display:flex;
    flex-wrap:wrap;
}
.feed_small_img{
   
    margin:5px 5px;
}
.feed_foot{
    display:flex;
   justify-content: space-around;
}
.feed_text{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #666666;
}
.doFollow{
    position:fixed;
    bottom:0;
    width:100%;
    height:88rpx;
    background: rgba(238,238,238,0.80);
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    line-height:88rpx;
    color: #0047FF;
    text-align:center;
}
.user_follower{
    margin-left:10rpx;
}
